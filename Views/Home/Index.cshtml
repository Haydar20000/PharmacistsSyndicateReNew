@model IndexVM
@{
    ViewData["Title"] = DkStrings.IndexTitle;
}

<div class="input-main">
    <h2 class="main-Title">@DkStrings.IndexMainComment01</h2>
    <h2 class="main-Title">@DkStrings.IndexMainComment02</h2>
    <form method="post" enctype="multipart/form-data">
        <div class="form-group-big">
            <label class="input-field-label-main" asp-for="FullName"></label>
            <input class="input-field-input-main" asp-for="FullName" />
        </div>
        <div class="form-group-big">
            <label class="input-field-label-main" asp-for="RegisterNumber"></label>
            <input class="input-field-input-main" asp-for="RegisterNumber" />
        </div>
        <div class="form-group-big">
            <label class="input-field-label-main" asp-for="Email"></label>
            <input class="input-field-input-main" asp-for="Email" />
        </div>
        <div class="form-group-big">
            <label class="input-field-label-main" asp-for="PhoneNumber"></label>
            <input class="input-field-input-main" asp-for="PhoneNumber" />
        </div>

        <div class="form-center" style="margin-top : 20px;">
            <button class="btn-style01" style="width:100px" type="submit">@DkStrings.CreateMembershipTitle</button>
            <a class="btn-style01" style="width:72px;padding:14px;text-align:center"
                href="~/SyndicateManagement/Create/@Model.MembershipId">@DkStrings.Back</a>
        </div>

    </form>
    @* <a class="btn-style01" style="width:150px;padding:14px;text-align:center; margin-top: 30px;"
        href="~/SyndicateManagement/Create">@DkStrings.CreateMembershipTitle</a> *@

</div>